{
  "name": "trinity-mobile",
  "version": "1.2.0",
  "private": true,
  "url": "https://trinity.iota.org",
  "homepage": "https://trinity.iota.org",
  "description": "Mobile wallet for IOTA",
  "author": "IOTA Foundation <contact@iota.org>",
  "license": "Apache-2.0 OR EPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iotaledger/trinity-wallet.git"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "log:android": "./node_modules/.bin/react-native log-android",
    "log:ios": "./node_modules/.bin/react-native log-ios",
    "android": "./node_modules/.bin/react-native run-android --appIdSuffix 'debug'",
    "android:dev": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/react-native run-android --appIdSuffix 'debug'",
    "ios": "./node_modules/.bin/react-native run-ios",
    "ios:dev": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/react-native run-ios",
    "test": "REALM_DISABLE_ANALYTICS=true NODE_ENV=test ./node_modules/.bin/jest --forceExit --detectOpenHandles --maxWorkers=10",
    "nodejs-deps": "yarn --cwd nodejs-assets/nodejs-project",
    "postinstall": "./node_modules/.bin/rn-nodeify --install crypto,http,os,path,stream,vm --hack --yarn && yarn nodejs-deps && patch-package && npm run fix-modal && react-native-schemes-manager all",
    "prepare-release": "yarn version && cd ios && bundle exec fastlane release && cd ../android && bundle exec fastlane release && cd .. && echo Successfully incremented version and build numbers! Please remember to run git add ., git commit, git push --tags, and git push",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "fix-modal": "node node_modules/react-native-modal-translucent/scripts/translucent-modal.js"
  },
  "xcodeSchemes": {
    "Debug": [],
    "Release": [
      "Beta"
    ]
  },
  "dependencies": {
    "@tradle/react-native-http": "^2.0.0",
    "axios": "^0.18.1",
    "bugsnag-react-native": "^2.22.0",
    "events": "^1.1.1",
    "hoist-non-react-statics": "^2.3.1",
    "intl": "^1.2.5",
    "jsc-android": "241213.x.x",
    "lodash": "^4.17.13",
    "lottie-react-native": "^2.6.1",
    "nodejs-mobile-react-native": "https://github.com/rajivshah3/nodejs-mobile-react-native#f3e99d92aa6983ca84805839b58cbe2ad2a33a37",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.6.0",
    "punycode": "^1.4.1",
    "qr.js": "https://github.com/defunctzombie/qr.js",
    "react": "16.8.3",
    "react-i18next": "^11.0.0",
    "react-native": "0.59.10",
    "react-native-camera": "^1.5.1",
    "react-native-crypto": "^2.0.2",
    "react-native-detect-navbar-android": "^0.2.0",
    "react-native-device-info": "^0.13.0",
    "react-native-document-picker": "^3.2.2",
    "react-native-dropdownalert": "https://github.com/cvarley100/react-native-dropdownalert#fc6655c4542e3b35da67ccca5f708adb0a7ccff8",
    "react-native-exit-app": "^1.0.0",
    "react-native-extra-dimensions-android": "^0.21.0",
    "react-native-fingerprint-scanner": "^2.3.1",
    "react-native-flag-secure-android": "git+https://github.com/kristiansorens/react-native-flag-secure-android#master",
    "react-native-fs": "^2.8.1",
    "react-native-google-safetynet": "^0.3.3",
    "react-native-haptic-feedback": "^1.4.0",
    "react-native-http": "github:tradle/react-native-http#834492d",
    "react-native-is-device-rooted": "https://github.com/rajivshah3/react-native-isDeviceRooted#latest",
    "react-native-keep-awake": "^2.0.6",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-keychain": "3.0.0",
    "react-native-markdown-renderer": "^3.2.1",
    "react-native-modal": "^11.4.0",
    "react-native-modal-translucent": "^2.0.0",
    "react-native-navigation": "^2.22.3",
    "react-native-optimized-flatlist": "^1.0.4",
    "react-native-os": "git+https://github.com/laumair/react-native-os.git#527b0ae",
    "react-native-picker-select": "^6.3.3",
    "react-native-print": "^0.5.0",
    "react-native-progress": "^3.4.0",
    "react-native-qr-generator": "^1.0.3",
    "react-native-qr-scanner": "https://github.com/cvarley100/react-native-qr-scanner#867e45d7f46d8464a636d85a0250d9e409c7beb6",
    "react-native-randombytes": "^3.2.0",
    "react-native-safe-area-view": "^0.9.0",
    "react-native-securerandom": "^0.1.1",
    "react-native-share": "^1.0.27",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^9.5.3",
    "react-native-timer": "^1.3.6",
    "react-native-triangle": "0.0.8",
    "react-native-vector-icons": "^6.4.1",
    "react-native-view-shot": "git+https://github.com/cvarley100/react-native-view-shot#master",
    "react-native-webview": "5.8.2",
    "react-redux": "^5.0.6",
    "readable-stream": "^1.0.33",
    "realm": "2.29.0",
    "redux": "^3.7.2",
    "rn-fetch-blob": "https://github.com/rajivshah3/rn-fetch-blob#10.13-226",
    "shared-modules": "../shared/",
    "snyk": "^1.217.3",
    "stream": "0.0.2",
    "stream-browserify": "^1.0.0",
    "text-encoding": "^0.6.4",
    "tinycolor2": "^1.4.1",
    "tweetnacl": "^1.0.0",
    "tweetnacl-util": "^0.15.0",
    "util": "^0.12.1",
    "victory-native": "^32.0.2",
    "vm-browserify": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-stage-0": "^7.0.0-beta",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "jest": "^24.8.0",
    "jest-serializer-enzyme": "^1.0.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "patch-package": "^6.1.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-native-schemes-manager": "^2.0.0",
    "react-test-renderer": "16.8.3",
    "rn-nodeify": "^10.0.1"
  },
  "resolutions": {
    "js-yaml": ">=3.13.1",
    "handlebars": ">=4.1.2",
    "set-value": ">=2.0.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./testConfig.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "./node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|shared-modules)/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/ios/build/",
      "<rootDir>/android/build/",
      "<rootDir>/nodejs-assets/"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "http": "@tradle/react-native-http",
    "os": "react-native-os",
    "path": "path-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "http": "@tradle/react-native-http",
    "os": "react-native-os",
    "path": "path-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "rnpm": {
    "android": {
      "mainFilePath": "src/main/java/com/iota/trinity/MainApplication.java"
    }
  },
  "snyk": true
}
